#: ../src/main.c:46
#, c-format
msgid "%s encrypts a paradox file."
msgstr "%s verschlüsselt eine Paradox-Datei."

#: ../src/main.c:48
#, c-format
msgid "%s decrypts a paradox file."
msgstr "%s entschlüsselt eine Paradox-Datei."

#: ../src/main.c:50
#, c-format
msgid "%s decrypts or encrypts a paradox file."
msgstr "%s entschlüsselt oder verschlüsselt eine Paradox-Datei."

#: ../src/main.c:53
#, c-format
msgid "Usage: %s [OPTIONS] FILE"
msgstr "Aufruf: %s [OPTION] DATEI"

#: ../src/main.c:55
#, c-format
msgid "General options:"
msgstr "Allgemeine Optionen:"

#: ../src/main.c:57
#, c-format
msgid "  -h, --help          this usage information."
msgstr "  -h, --help          diese Hilfe."

#: ../src/main.c:59
#, c-format
msgid "  --version           show version information."
msgstr "  --version           zeige Programmversion."

#: ../src/main.c:61
#, c-format
msgid "  -v, --verbose       be more verbose."
msgstr "  -v, --verbose       mehr Informationen anzeigen."

#: ../src/main.c:65
#, c-format
msgid "  --use-gsf           use gsf library to read input file."
msgstr ""
"  --use-gsf           benutze gsf-Bibliothek zum Lesen der Eingabedatei."

#: ../src/main.c:70
#, c-format
msgid "  -o, --output-file=FILE output data into file instead of stdout."
msgstr "  -o, --output-file=DATEI schreibe Ausgabe in Datei statt nach stdout."

#: ../src/main.c:73
#, c-format
msgid "Options to select mode:"
msgstr "Optionen, die den Modus betreffen:"

#: ../src/main.c:75
#, c-format
msgid "  --mode=MODE         set operation mode (encrypt, decrypt)."
msgstr "  --mode=MODE         setze Modus (encrypt, decrypt)."

#: ../src/main.c:77
#, c-format
msgid "  -e, --encrypt       encrypt file."
msgstr "  -e, --encrypt           verschlüssel Datei."

#: ../src/main.c:79
#, c-format
msgid "  -d, --decrypt       decrypt file."
msgstr "  -d, --decrypt           entschlüssel Datei."

#: ../src/main.c:84
#, c-format
msgid "Encryption options:"
msgstr "Optionen für die Verschlüsselung"

#: ../src/main.c:86
#, c-format
msgid "  --password=WORD     set password for encryption."
msgstr "  --password=WORD     setze Passwort für Verschlüsselung."

#: ../src/main.c:91
#, c-format
msgid "libpx has been compiled for big endian architecture."
msgstr "libpx wurde für Architekturen mit big endian übersetzt."

#: ../src/main.c:93
#, c-format
msgid "libpx has been compiled for little endian architecture."
msgstr "libpx wurde für Architekturen mit little endian übersetzt."

#: ../src/main.c:95
#, c-format
msgid "libpx has gsf support: %s"
msgstr "libpx hat Unterstützung für die gsf-Bibliothek: %s"

#: ../src/main.c:95
msgid "Yes"
msgstr "Ja"

#: ../src/main.c:95
msgid "No"
msgstr "Nein"

#: ../src/main.c:97
#, c-format
msgid "libpx has version: %d.%d.%d"
msgstr "libpx ist Version: %d.%d.%d"

#: ../src/main.c:208
#, c-format
msgid "You must at least specify an input file."
msgstr "Sie müssen mindestes eine Eingabedatei angegeben."

#: ../src/main.c:232
#, c-format
msgid "Encryption mode requires a password."
msgstr "Verschlüsselungsmodus erfordert Angabe eines Passworts."

#: ../src/main.c:245
#, c-format
msgid "Could not open output file."
msgstr "Konnte Ausgabedatei nicht öffnen."

#: ../src/main.c:259
#, c-format
msgid "Could not create new paradox instance."
msgstr "Konnte eine neue Paradox-Instanz nicht anlegen."

#: ../src/main.c:278
#, c-format
msgid "Could not open gsf input file."
msgstr "Konnte Eingabedatei mit gsf-Bibliothek nicht öffnen."

#: ../src/main.c:287 ../src/main.c:294
#, c-format
msgid "Could not open input file."
msgstr "Sie müssen mindestes eine Eingabedatei angegeben."

#: ../src/main.c:323
#, c-format
msgid "Input file is already encrypted."
msgstr "Eingabedatei ist bereits verschlüsselt."

#: ../src/main.c:333 ../src/main.c:423
