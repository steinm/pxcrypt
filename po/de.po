#: ../src/main.c:45
#, c-format
msgid "%s decrypts or encrypts a paradox file."
msgstr "%s entschlüsselt oder verschlüsselt eine Paradox datei."

#: ../src/main.c:47
#, c-format
msgid "Usage: %s [OPTIONS] FILE"
msgstr "Aufruf: %s [OPTION] DATEI"

#: ../src/main.c:49
#, c-format
msgid "General options:"
msgstr "Allgemeine Optionen:"

#: ../src/main.c:51
#, c-format
msgid "  -h, --help          this usage information."
msgstr "  -h, --help          diese Hilfe."

#: ../src/main.c:53
#, c-format
msgid "  --version           show version information."
msgstr "  --version           zeige Programmversion."

#: ../src/main.c:55
#, c-format
msgid "  -v, --verbose       be more verbose."
msgstr "  -v, --verbose       mehr Informationen anzeigen."

#: ../src/main.c:59
#, c-format
msgid "  --use-gsf           use gsf library to read input file."
msgstr ""
"  --use-gsf           benutze gsf-Bibliothek zum Lesen der Eingabedatei."

#: ../src/main.c:64
#, c-format
msgid "  -o, --output-file=FILE output data into file instead of stdout."
msgstr "  -o, --output-file=DATEI schreibe Ausgabe in Datei statt nach stdout."

#: ../src/main.c:66
#, c-format
msgid "Options to select mode:"
msgstr "Optionen, die den Modus betreffen:"

#: ../src/main.c:68
#, c-format
msgid "  --mode=MODE         set operation mode (encrypt, decrypt)."
msgstr "  --mode=MODE         setze Modus (encrypt, decrypt)."

#: ../src/main.c:70
#, c-format
msgid "  -e, --encrypt       encrypt file."
msgstr "  -e, --encrypt           verschlüssel Datei."

#: ../src/main.c:72
#, c-format
msgid "  -d, --decrypt       decrypt file."
msgstr "  -d, --decrypt           entschlüssel Datei."

#: ../src/main.c:75
#, c-format
msgid "Encryption/decryption options:"
msgstr "Optionen für die Ver/Entschlüsselung"

#: ../src/main.c:77
#, c-format
msgid "  --password=WORD     set password for encyption."
msgstr "  --password=WORD     setze Passwort für Verschlüsselung."

#: ../src/main.c:81
#, c-format
msgid "libpx has been compiled for big endian architecture."
msgstr "libpx wurde für Architekturen mit big endian übersetzt."

#: ../src/main.c:83
#, c-format
msgid "libpx has been compiled for little endian architecture."
msgstr "libpx wurde für Architekturen mit little endian übersetzt."

#: ../src/main.c:85
#, c-format
msgid "libpx has gsf support: %s"
msgstr "libpx hat Unterstützung für die gsf-Bibliothek: %s"

#: ../src/main.c:85
msgid "Yes"
msgstr "Ja"

#: ../src/main.c:85
msgid "No"
msgstr "Nein"

#: ../src/main.c:87
#, c-format
msgid "libpx has version: %d.%d.%d"
msgstr "libpx ist Version: %d.%d.%d"

#: ../src/main.c:194
#, c-format
msgid "You must at least specify an input file."
msgstr "Sie müssen mindestes eine Eingabedatei angegeben."

#: ../src/main.c:218
#, c-format
msgid "Encryption mode requires a password."
msgstr "Verschlüsselungsmodus erfordert Angabe eines Passworts."

#: ../src/main.c:231
#, c-format
msgid "Could not open output file."
msgstr "Konnte Ausgabedate nicht öffnen."

#: ../src/main.c:245
#, c-format
msgid "Could not create new paradox instance."
msgstr "Konnte eine neue Paradox-Instanz nicht anlegen."

#: ../src/main.c:264
#, c-format
msgid "Could not open gsf input file."
msgstr "Konnte Eingabedatei mit gsf-Bibliothek nicht öffnen."

#: ../src/main.c:273 ../src/main.c:280
#, c-format
msgid "Could not open input file."
msgstr "Sie müssen mindestes eine Eingabedatei angegeben."

#: ../src/main.c:309
#, c-format
msgid "Input file is already encrypted."
msgstr "Eingabedatei ist bereits verschlüsselt."

#: ../src/main.c:319 ../src/main.c:409
msgid "Could not allocate memory for header of input file."
msgstr "Konnte Speicher für Kopf der Eingabedatei nicht allozieren."

#: ../src/main.c:325 ../src/main.c:415
#, c-format
msgid "Could not seek start of input file."
msgstr "Konnte nicht an den Anfang der Eingabedatei springen."

#: ../src/main.c:333 ../src/main.c:423
#, c-format
msgid "Could not read header of input file."
msgstr "Konnte Kopf der Eingabedatei nicht lesen."

#: ../src/main.c:344 ../src/main.c:434
#, c-format
msgid "Could not write header to output file."
msgstr "Konnte Kopf nicht in Ausgabedate schreiben."

#: ../src/main.c:355 ../src/main.c:445
msgid "Could not allocate memory for block of input file."
msgstr "Konnte Speicher für Datenblock der Eingabedatei nicht allozieren."

#: ../src/main.c:364 ../src/main.c:454
#, c-format
msgid "File has %d data blocks."
msgstr "Datei hat %d Daten-blöcke."

#: ../src/main.c:369 ../src/main.c:459
#, c-format
msgid "Could not read block from input file."
msgstr "Konnte Datenblock aus Eingabedatei nicht lesen."

#: ../src/main.c:377
#, c-format
msgid "Writing block %d."
msgstr "Schreibe Datenblock %d."

#: ../src/main.c:382 ../src/main.c:468
#, c-format
msgid "Could not write block to output file."
msgstr "Konnte Datenblock nicht in Ausgabedate schreiben."

#: ../src/main.c:399
#, c-format
msgid "Input file is not encrypted."
msgstr "Eingabedatei ist nicht verschlüsselt."
